/**
 * 删除文件名中的空格
 */
    /**
     * 前提条件，将目录中的空格删除，顺序不能颠倒
     */
    find ./ -mindepth 1 -maxdepth 1 -type d |  xargs -n 1 -t -d '\n' rename 's/ /_/g'
    find ./ -mindepth 2 -maxdepth 2 -type d |  xargs -n 1 -t -d '\n' rename 's/ /_/g'
    find ./ -mindepth 3 -maxdepth 3 -type d |  xargs -n 1 -t -d '\n' rename 's/ /_/g'

    find ./ -depth | grep -iv x-art | grep -iv jpg | grep -iv mywife |  xargs -n 1 -t -d '\n' rename 's/ //g'
    find ./ -depth |  xargs -n 1 -t -d '\n' rename 's/ /_/g'
    find ./ -depth |  xargs -n 1 -t -d '\n' rename 's/ //g'
    find ./ -depth |  xargs -n 1 -t -d '\n' rename 's/，/_/g'
    find ./ -depth |  xargs -n 1 -t -d '\n' rename 's/，//g'
/**
 * 将中文"，"修改为"_"
 */
    find ./ |  xargs -n 1 -t -d '\n' rename 's/，/_/g'

/**
 * 将MP4文件上移到前一个文件夹
 */
    find ./ -name "*.mp4" | xargs -t -I {} mv {} ./
/**
 * 将文件上移到执行命令的当前文件夹
 */
    find ./ -type f | xargs -t -I {} mv {} ./
/**
 * 删除文件夹
 */
    find ./ -type d | xargs -t -I {} rm -rf {} 

/**
 * "Q28017"目录下的文件不做任何动作
 */
     find ./ \( -path "./Q28017" \) -prune -o -name "*.png" | xargs -t -I {} rm -f {}
