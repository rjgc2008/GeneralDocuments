iptables -A INPUT -p tcp --dport 22 -m state --state NEW -m recent --set --name SSH
iptables -A INPUT -p tcp --dport 22 -m state --state NEW -m recent --update --seconds 60 --hitcount 5 --name SSH -j DROP


iptable初始:
iptables -A INPUT -s 192.168.31.0/24 -j ACCEPT
iptables -A INPUT -s 113.125.74.234/32 -j ACCEPT
iptables -A INPUT -p tcp --dport 22 -m state --state NEW -m recent --set --name SSH
iptables -A INPUT -p tcp --dport 22 -m state --state NEW -m recent --update --seconds  3600 --hitcount 3 --name SSH -j LOG --log-prefix 'SSH Attack'
iptables -A INPUT -p tcp --dport 22 -m state --state NEW -m recent --update --seconds  3600 --hitcount 3 --name SSH -j DROP


iptables进阶版本：
    Beta1:
    iptables -A INPUT -s 192.168.31.0/24 -j ACCEPT
    iptables -A INPUT -s 113.125.74.234/32 -j ACCEPT
    iptables -A INPUT -m state --state RELATED,ESTABLISHED -j ACCEPT
    iptables -A INPUT -p icmp --icmp-type 8 -m length --length 78 -j LOG --log-prefix 'SSH_OPEN_KEY'
    iptables -A INPUT -p icmp --icmp-type 8 -m length --length 78 -m recent --name openssh --set --rsource 
    iptables -A INPUT -p tcp --dport 22  --syn -m recent --name openssh --rcheck  --rsource -j ACCEPT


    Beta2:
    iptables -A INPUT -s 192.168.31.0/24 -j ACCEPT
    iptables -A INPUT -s 113.125.74.234/32 -j ACCEPT
    iptables -A INPUT -p tcp --dport 22 -j LOG --log-prefix 'SSH DROP'
    iptables -A INPUT -p tcp --dport 22 -j DROP
    iptables -A INPUT -p tcp --dport 5900 -j LOG --log-prefix 'VNC DROP'






    iptables -I INPUT -p icmp --icmp-type 8 -s 0/0 -j DROP

58.208.145.211

    iptables -R INPUT 6 -p tcp --dport 22  --syn -m recent --name openssh --rcheck  --rsource -j ACCEPT


    iptables -A INPUT -p icmp --icmp-type 8  -j LOG --log-prefix 'SSH_OPEN_KEY'

    iptables -R INPUT 5  -p icmp --icmp-type 8  -m recent --name openssh --set --rsource 







